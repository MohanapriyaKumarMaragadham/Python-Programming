##operations only with integers##

a=5
b=10
c=156
d=27
print a+b
print c+d
a=5
b=16
c=15
d=27
print a-b
print c-d
a=5
b=110
c=15
d=27
print a*b
print c*d
a=5
b=10
c=156
d=27
print a/b
print c/d

##Float operations

a=5.0
b=10.0
c=15.0
d=27.0
print a+b
print c+d
a=6.0
b=12.0
c=156.0
d=27.025
print a-b
print c-d
a=99.0
b=15.0
c=15.0
d=278.0
print a*b
print c*d
a=5.0
b=16.0
c=199.0
d=29.0
print a/b
print c/d
